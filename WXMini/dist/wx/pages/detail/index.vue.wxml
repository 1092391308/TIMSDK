<template name="8b0f2e64">
  <view class="_div data-v-31e8b46d bg">
    <view class="_div data-v-31e8b46d card">
      <i-row class="_i-row data-v-31e8b46d">
        <i-col span="8" class="_i-col data-v-31e8b46d">
          <view class="_div data-v-31e8b46d avatar">
            <image style="width: 80px; height: 80px; border-radius: 8px" src="/static/images/header.png" class="_image data-v-31e8b46d"></image>
          </view>
        </i-col>
        <i-col span="16" class="_i-col data-v-31e8b46d">
          <view class="_div data-v-31e8b46d right">
            <view class="_div data-v-31e8b46d username">{{userProfile.nick || '未设置'}}</view>
            <view class="_div data-v-31e8b46d account">账号：{{userProfile.userID}}</view>
          </view>
        </i-col>
      </i-row>
    </view>
    <view class="_div data-v-31e8b46d card" style="margin-top:20px">
      <view class="_div data-v-31e8b46d item">
        <view class="_div data-v-31e8b46d key">性别</view>
        <view clasa="value" class="_div data-v-31e8b46d">{{userProfile.gender || '未设置'}}</view>
      </view>
      <view class="_div data-v-31e8b46d item">
        <view class="_div data-v-31e8b46d key">生日</view>
        <view class="_div data-v-31e8b46d value">{{userProfile.birthday || '未设置'}}</view>
      </view>
      <view class="_div data-v-31e8b46d item">
        <view class="_div data-v-31e8b46d key">地址</view>
        <view class="_div data-v-31e8b46d value">{{userProfile.location || '未设置'}}</view>
      </view>
      <view class="_div data-v-31e8b46d item">
        <view class="_div data-v-31e8b46d key">签名</view>
        <view class="_div data-v-31e8b46d value">{{userProfile.selfSignature || '未设置'}}</view>
      </view>
    </view>
    <view class="_div data-v-31e8b46d revise"><button bindtap="handleProxy" class="_button data-v-31e8b46d btn delete" wx:if="{{!isInBlacklist}}" data-eventid="{{'0'}}" data-comkey="{{$k}}">拉黑</button> <button bindtap="handleProxy" class="_button data-v-31e8b46d btn" wx:if="{{isInBlacklist}}" data-eventid="{{'1'}}" data-comkey="{{$k}}">取消拉黑</button></view>
  </view>
</template>