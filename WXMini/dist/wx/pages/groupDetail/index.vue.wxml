<template name="612ccb22">
  <view class="_div data-v-636b852a bg">
    <i-modal title="添加的用户ID" visible="{{addModalVisible}}" bindok="handleProxy" bindcancel="handleProxy" data-eventid="{{'1'}}" data-comkey="{{$k}}" class="_i-modal data-v-636b852a">
      <view class="_div data-v-636b852a input-wrapper"><input type="text" class="_input data-v-636b852a input" value="{{addUserId}}" bindinput="handleProxy" data-eventid="{{'0'}}" data-comkey="{{$k}}" /></view>
    </i-modal>
    <i-modal title="转让群组" visible="{{changeOwnerModal}}" bindok="handleProxy" bindcancel="handleProxy" data-eventid="{{'3'}}" data-comkey="{{$k}}" class="_i-modal data-v-636b852a">
      <view class="_div data-v-636b852a input-wrapper">
        <i-radio-group current="{{current}}" bindchange="handleProxy" data-eventid="{{'2'}}" data-comkey="{{$k}}" class="_i-radio-group data-v-636b852a">
          <i-radio position="left" wx:key="item.userID" key="{{item.userID}}" value="{{item.userID}}" wx:for="{{groupProfile.memberList}}" wx:for-index="index" wx:for-item="item" class="_i-radio data-v-636b852a">{{item.userID}}
          </i-radio>
        </i-radio-group>
      </view>
    </i-modal>
    <view class="_div data-v-636b852a card">
      <i-row class="_i-row data-v-636b852a">
        <i-col span="8" class="_i-col data-v-636b852a">
          <view class="_div data-v-636b852a avatar">
            <image style="width: 80px; height: 80px; border-radius: 8px" src="{{groupProfile.avatar || '/static/images/groups.png'}}" class="_image data-v-636b852a"></image>
          </view>
        </i-col>
        <i-col span="16" class="_i-col data-v-636b852a">
          <view class="_div data-v-636b852a right">
            <view class="_div data-v-636b852a username">{{groupProfile.name}}</view>
            <view class="_div data-v-636b852a account">群ID：{{groupProfile.groupID}}</view>
          </view>
        </i-col>
      </i-row>
    </view>
    <view class="_div data-v-636b852a card" style="margin-top:10px;padding-left:20px;padding-right:20px ">
      <i-row class="_i-row data-v-636b852a">
        <i-col span="22" class="_i-col data-v-636b852a">
          <view class="_div data-v-636b852a member"> 群成员 </view>
        </i-col>
        <i-col span="2" class="_i-col data-v-636b852a">
          <view class="_div data-v-636b852a member" bindtap="handleProxy" data-eventid="{{'4'}}" data-comkey="{{$k}}">
            <image style="width:20px;height:20px;border-radius:50%" src="/static/images/right.png" class="_image data-v-636b852a"></image>
          </view>
        </i-col>
      </i-row>
      <i-row class="_i-row data-v-636b852a">
        <i-col span="3" wx:if="{{currentGroupProfile.type === 'Private'}}" class="_i-col data-v-636b852a">
          <view class="_div data-v-636b852a avatar-list" bindtap="handleProxy" data-eventid="{{'5'}}" data-comkey="{{$k}}">
            <image style="width:30px;height:30px;border-radius:50%" src="/static/images/more.png" class="_image data-v-636b852a"></image>
          </view>
        </i-col>
        <i-col span="3" wx:key="index" key="{{index}}" wx:for="{{groupProfile.memberList}}" wx:for-index="index" wx:for-item="person" class="_i-col data-v-636b852a">
          <view class="_div data-v-636b852a avatar-list">
            <image style="width:30px;height:30px;border-radius:50%" src="{{person.avatar || '/static/images/header.png'}}" class="_image data-v-636b852a"></image>
          </view>
        </i-col>
      </i-row>
    </view>
    <view class="_div data-v-636b852a card">
      <view class="_div data-v-636b852a item">
        <view class="_div data-v-636b852a key">群介绍</view>
        <view class="_div data-v-636b852a value">{{groupProfile.introduction || '未设置'}}</view>
      </view>
    </view>
    <view class="_div data-v-636b852a card">
      <view class="_div data-v-636b852a item">
        <view class="_div data-v-636b852a key">群公告</view>
        <view class="_div data-v-636b852a value">{{groupProfile.notification || '未设置'}}</view>
      </view>
    </view>
    <view class="_div data-v-636b852a card">
      <view class="_div data-v-636b852a item">
        <view class="_div data-v-636b852a key">群类型</view>
        <view class="_div data-v-636b852a value">{{groupProfile.type}}</view>
      </view>
    </view>
    <view class="_div data-v-636b852a revise" wx:if="{{isOwner && groupProfile.type !== 'AVChatRoom'}}"><button bindtap="handleProxy" class="_button data-v-636b852a btn delete" data-eventid="{{'6'}}" data-comkey="{{$k}}">转让群组</button></view>
    <view class="_div data-v-636b852a revise" wx:if="{{groupProfile.type !== 'Public'}}"><button bindtap="handleProxy" class="_button data-v-636b852a btn delete" data-eventid="{{'7'}}" data-comkey="{{$k}}">退出群组</button></view>
    <view class="_div data-v-636b852a revise" wx:if="{{isOwner && groupProfile.type !== 'Private'}}"><button bindtap="handleProxy" class="_button data-v-636b852a btn delete" data-eventid="{{'8'}}" data-comkey="{{$k}}">解散群组</button></view>
  </view>
</template>